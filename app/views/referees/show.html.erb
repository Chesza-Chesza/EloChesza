<div class="container">  
  <p id="notice"><%= notice %></p>

  <p>
    <strong>Fide:</strong>
    <%= @referee.fide_id %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= @referee.name %>
  </p>

  <p>
    <strong>Title:</strong>
    <%= @referee.title %>
  </p>

  <%= link_to 'Edit', edit_referee_path(@referee), class: "btn btn-light rounded-pill" %> |
  <%= link_to 'Back', referees_path, class: "btn btn-light rounded-pill" %>
</div>
