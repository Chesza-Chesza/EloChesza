<div class="container">
  <p id="notice"><%= notice %></p>
  
  <h1>Gameplayers</h1>
  
  <div class="table-responsive">
    <table class="table table-striped table-sm">
      <thead class="thead-dark">
        <tr>
          <th>Elo</th>
          <th>Rtng change</th>
          <th>Game</th>
          <th>Player</th>
          <th colspan="3"></th>
        </tr>
      </thead>
  
    <tbody>
      <% @gameplayers.each do |gameplayer| %>
        <tr>
          <td><%= gameplayer.elo %></td>
          <td><%= gameplayer.rtng_change %></td>
          <td><%= gameplayer.game_id %></td>
          <td><%= gameplayer.player_id %></td>
          <td><%= link_to 'Show', gameplayer %></td>
          <td><%= link_to 'Edit', edit_gameplayer_path(gameplayer) %></td>
          <td><%= link_to 'Destroy', gameplayer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  
  <br>
  
  <%= link_to 'New Gameplayer', new_gameplayer_path, class: 'btn btn-dark rounded mb-4' %>
</div>
